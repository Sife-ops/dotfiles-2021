#^---- wyattgoettsch@gmx.us ----------------------------------------------------
[Account wyattgoettschatgmxdotus]
# Identifier for the local repository; e.g. the maildir to be synced via IMAP.
localrepository = wyattgoettschatgmxdotus-local
# Identifier for the remote repository; i.e. the actual IMAP, usually non-local.
remoterepository = wyattgoettschatgmxdotus-remote

[Repository wyattgoettschatgmxdotus-local]
# OfflineIMAP supports Maildir, GmailMaildir, and IMAP for local repositories.
type = Maildir
# Where should the mail be placed?
localfolders = ~/.local/share/mail/wyattgoettsch@gmx.us

[Repository wyattgoettschatgmxdotus-remote]
# Remote repos can be IMAP or Gmail, the latter being a preconfigured IMAP.
type = IMAP
remotehost = imap.gmx.com
remoteuser = wyattgoettsch@gmx.us
remotepasseval = get_pass('wyattgoettschatgmxdotus.gpg')
sslcacertfile = /etc/ssl/certs/ca-certificates.crt
#$

